@model IEnumerable<Vehicle>

<h2>Vehicles</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table>
    <tr>
        <th>Id</th>
        <th>Car Plate</th>
        <th>Km</th>
        <th>Model</th>
        <th>Color</th>
        <th>Gear Type</th>
        <th>Brand</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.CarPlate</td>
            <td>@item.Km</td>
            <td>@item.Model</td>
            <td>@item.Color</td>
            <td>@item.GearType</td>
            <td>@item?.Brand?.Name</td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
    }
</table>

